  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix"
  },
  "dependencies": {
    "fastify-plugin": "^3.0.0",
    "permissions-policy": "^0.6.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.10",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
