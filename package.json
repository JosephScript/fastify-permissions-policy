{
  "name": "fastify-permissions-policy",
  "version": "1.0.2",
  "description": "Fastify plugin for setting custom permissions policies. ",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
