    "@types/node": "^15.0.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint": "^7.30.0",
    "fastify": "^3.0.0",
    "husky": "^7.0.0",
